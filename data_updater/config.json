{
  "patients": {
    "data": [
      {
        "data_count": "the_number_of_patients_with_symptoms.yaml",
        "data_details": "details_of_patients_with_symptoms.yaml",
        "raw": "raw_patients_with_symptoms.tsv",
        "table_index": 0
      },
      {
        "data_count": "the_number_of_patients_without_symptoms.yaml",
        "data_details": "details_of_patients_without_symptoms.yaml",
        "raw": "raw_patients_without_symptoms.tsv",
        "table_index": 1
      }
    ],
    "dst": "./data/",
    "formats": {
      "keys": {
        "age": [
          {
            "find": "10歳未満",
            "replace": "under_teens"
          },
          {
            "find": "10代",
            "replace": "teens"
          },
          {
            "find": "20代",
            "replace": "twenties"
          },
          {
            "find": "30代",
            "replace": "thirties"
          },
          {
            "find": "40代",
            "replace": "forties"
          },
          {
            "find": "50代",
            "replace": "fifties"
          },
          {
            "find": "60代",
            "replace": "sixties"
          },
          {
            "find": "70代",
            "replace": "seventies"
          },
          {
            "find": "80代",
            "replace": "eighties"
          },
          {
            "find": "90代",
            "replace": "nineties"
          },
          {
            "find": "90代以上",
            "replace": "over_nineties"
          }
        ]
      },
      "values": {
        "ichikawa": [
          {
            "find": "\\D+",
            "replace": ""
          }
        ],
        "chiba": [
          {
            "find": "^\\D.+$",
            "replace": ""
          },
          {
            "find": "^(\\d+)\\D+$",
            "replace": "\\1"
          }
        ],
        "inspection_date": [
          {
            "find": "^不明|調査中|\\s+|None$",
            "replace": ""
          },
          {
            "find": "(\\d+)月(\\d+)日",
            "replace": "2020-\\1-\\2"
          }
        ],
        "onset_date": [
          {
            "find": "^不明|調査中|\\s+|None$",
            "replace": ""
          },
          {
            "find": "(\\d+)月(\\d+)日",
            "replace": "2020-\\1-\\2"
          }
        ],
        "age": [
          {
            "find": "^不明|調査中|\\s+|None$",
            "replace": ""
          }
        ],
        "sex": [
          {
            "find": "^不明|調査中|\\s+|None$",
            "replace": ""
          }
        ],
        "occupation": [
          {
            "find": "^不明|調査中|\\s+|None$",
            "replace": ""
          }
        ],
        "infection_sources": [
          {
            "find": "^不明|調査中|\\s+|None$",
            "replace": ""
          },
          {
            "find": "(.)接触",
            "replace": "\\1"
          },
          {
            "find": "、",
            "replace": ","
          }
        ],
        "activities": [
          {
            "find": "^不明|調査中|\\s+|None$",
            "replace": ""
          },
          {
            "find": "、",
            "replace": ","
          }
        ]
      }
    },
    "src": "https://www.city.ichikawa.lg.jp/pub01/hasseijokyo.html"
  },
  "subway": {
    "data": [
      {
        "data_passengers": "changes_in_the_rates_of_toei_subway_passengers.json",
        "raw": "raw_changes_in_the_rates_of_toei_subway_passengers.json"
      }
    ],
    "dst": "./data/",
    "src": "https://raw.githubusercontent.com/tokyo-metropolitan-gov/covid19/development/data/metro.json"
  }
}
